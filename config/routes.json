{
    "": {
        "controller": "Default",
        "method": "start"
    },
    "login": {
        "controller": "User",
        "method": "login"
    },
    "posts": {
        "controller": "Post",
        "method": "getAll"
    },
    "posts/:page": {
        "controller": "Post",
        "method": "getAllWithPage",
        "params": {
          "page": "number"
        }
      },
    "post/:id": {
        "controller": "Post",
        "method": "getOne",
            "params": {
                "id": "number"
            }
    }
}